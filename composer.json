{
    "name": "1tomany/oauth2-intuit",
    "description": "Intuit OAuth2 provider for The PHP League OAuth2 Client package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vic@1tomany.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "oauth2",
        "intuit",
        "quickbooks"
    ],
    "require": {
        "php": ">=8.3",
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\OAuth2\\Intuit\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\OAuth2\\Intuit\\Tests\\": "tests/"
        }
    }
}
